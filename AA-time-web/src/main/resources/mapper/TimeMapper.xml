<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ctg.aatime.dao.TimeDao">
    <insert id="insertTimeList">
        INSERT INTO time (<include refid="key"/>) VALUES (<include refid="value"/>)
    </insert>
    <sql id="key">
        <trim suffixOverrides=",">
            <if test="times[0].eventId != null">event_id,</if>
            <if test="times[0].uid != null">uid,</if>
            <if test="times[0].startTime != null">start_time,</if>
            <if test="times[0].endTime != null">end_time</if>
        </trim>
    </sql>
    <sql id="value">
        <foreach collection="times" item="item" index="index" separator=",">
            <if test="item.eventId != null">#{item.eventId},</if>
            <if test="item.uid != null">#{item.uid},</if>
            <if test="item.startTime != null">#{item.startTime},</if>
            <if test="item.endTime != null">#{item.endTime},</if>
        </foreach>
    </sql>
</mapper>